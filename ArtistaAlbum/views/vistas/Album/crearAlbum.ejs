<div class="page-header" >
  <h1 align="center">CREAR ALBUM</h1>
</div>
<form action="/Album/crearAlbum" method="post">

  <div>
    <select class="form-control">
      <% for(var i=0; i<artistas.length; i++) {%>
      <option><%= artistas[i].nombre %></option>
      <% }%>
    </select>
  </div>

  <div class="form-group">
    <label>Nombre:</label>
    <input type="text" class="form-control" placeholder="Ingrese el nombre del album" name="nombre">
  </div>

  <div class="form-group">
    <label>Fecha de lanzamiento</label>
    <input type="date" class="form-control" placeholder="Ingrese la fecha de la nzamiento"  name="fechaLanzamiento">
  </div>

  <div class="form-group">
    <label>URL de la Portada</label>
    <input type="text" class="form-control" placeholder="Ingrese el pais de recidencia"  name="URLPortada">
  </div>

  <button type="submit" class="btn btn-success">Crear Album</button>
</form>
